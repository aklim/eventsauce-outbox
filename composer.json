{
    "name": "andreo/eventsauce-outbox",
    "description": "Extended message outbox components for EventSauce.",
    "keywords": [
        "eventsauce",
        "components",
        "outbox",
        "message"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrzej Pakula",
            "email": "andrze.pakula@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "eventsauce/eventsauce": "^1.2",
        "eventsauce/message-outbox-for-doctrine": "^0.2.1",
        "eventsauce/test-utilities": "dev-main",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "eventsauce/uuid-encoding": "0.3.x-dev",
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.4",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Andreo\\EventSauce\\Outbox\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
